<?xml version="1.0" encoding="utf-8"?>
<MediaBackup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <PushoverUserKey>InsertYourPushoverUserKeyHere</PushoverUserKey>
  <PushoverAppToken>InsertYourPushoverAppTokenHere</PushoverAppToken>
  <!-- When true a backup scan will run when the app starts -->
  <StartScheduledBackup>false</StartScheduledBackup>
  <!-- When true the service monitoring starts when the app starts -->
  <StartMonitoring>true</StartMonitoring>
  <!-- A bigger percentage difference than this and the backup stops -->
  <DifferenceInFileCountAllowedPercentage>2</DifferenceInFileCountAllowedPercentage>
  <!--  ScheduledBackupStartTime as hh:mm -->
  <ScheduledBackupStartTime>11:00</ScheduledBackupStartTime>
  <!-- MinimumCriticalMasterFolderSpace in MB -->
  <MinimumCriticalMasterFolderSpace>200</MinimumCriticalMasterFolderSpace>
  <!-- MinimumCriticalBackupDiskSpace in MB -->
  <MinimumCriticalBackupDiskSpace>1</MinimumCriticalBackupDiskSpace>
  <!-- After this many days the backup warns to check the disks again -->
  <DaysToReportOldBackupDisks>90</DaysToReportOldBackupDisks>
  <!-- MinimumFreeSpaceToLeaveOnBackupDisk in MB -->
  <MinimumFreeSpaceToLeaveOnBackupDisk>10</MinimumFreeSpaceToLeaveOnBackupDisk>
  <!-- Minimum read speed in MB/s -->
  <MinimumMasterFolderReadSpeed>30</MinimumMasterFolderReadSpeed>
  <!-- Minimum write speed in MB/s -->
  <MinimumMasterFolderWriteSpeed>50</MinimumMasterFolderWriteSpeed>
  <!-- Monitor interval in seconds -->
  <MonitorInterval>60</MonitorInterval>
  <!-- If True we test the speed of the disks -->
  <DiskSpeedTests>false</DiskSpeedTests>
  <!-- Number of disk speed test iterations to run -->
  <SpeedTestIterations>2</SpeedTestIterations>
  <!-- Size of disk speed test file in MB -->
  <SpeedTestFileSize>200</SpeedTestFileSize>
  <!-- True to enable sending Pushover messages -->
  <StartSendingPushoverMessages>false</StartSendingPushoverMessages>
  <Monitors>
    <Monitor>
      <Url />
      <Timeout>30</Timeout>
      <ProcessToKill>plex media server</ProcessToKill>
      <ApplicationToStart>"%ProgramFiles(x86)%\Plex\Plex Media Server\Plex Media Server.exe"</ApplicationToStart>
      <Name>Plex</Name>
      <Port>0</Port>
    </Monitor>
  </Monitors>
  <MasterFolders>
    <!-- List each UNC path that you want to scan for files to backup here -->
    <MasterFolder>\\nas1\assets1\_Test</MasterFolder>
  </MasterFolders>
  <IndexFolders>
    <!-- These are the subfolders that will be searched in the shares above -->
    <IndexFolder>_Audio Books</IndexFolder>
    <IndexFolder>_Comedy</IndexFolder>
    <IndexFolder>_Concerts</IndexFolder>
    <IndexFolder>_MMA</IndexFolder>
    <IndexFolder>_Movies</IndexFolder>
    <IndexFolder>_Music</IndexFolder>
    <IndexFolder>_Other</IndexFolder>
    <IndexFolder>_Pictures</IndexFolder>
    <IndexFolder>_Playlists</IndexFolder>
    <IndexFolder>_Sport</IndexFolder>
    <IndexFolder>_TV</IndexFolder>
    <IndexFolder>_Videos</IndexFolder>
  </IndexFolders>
  <Filters>
    <!-- RegEx's of file to ignore when executing a scan -->
    <FilterRegEx>!*.bup</FilterRegEx>
  </Filters>
  <FilesToDelete>
    <FilesToDeleteRegEx>._.DS_Store</FilesToDeleteRegEx>
    <FilesToDeleteRegEx>.DS_Store</FilesToDeleteRegEx>
  </FilesToDelete>
  <DisksToSkipOnRestore />
  <BackupFiles />
  <BackupDisks>
    <BackupDisk>
      <Name>backup 1001</Name>
      <Checked>2023-08-15</Checked>
      <Capacity>11995171454976</Capacity>
      <TotalFiles>0</TotalFiles>
      <Free>318751838208</Free>
    </BackupDisk>
    <BackupDisk>
      <Name>backup 1002</Name>
      <Checked>2023-08-15</Checked>
      <Capacity>11995171454976</Capacity>
      <TotalFiles>1</TotalFiles>
      <Free>318751838208</Free>
    </BackupDisk>
  </BackupDisks>
</MediaBackup>